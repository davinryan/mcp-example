{
    "name": "mcp-example",
    "private": true,
    "version": "0.0.4",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "prepare": "husky",
        "pre-commit": "lint-staged",
        "build:client": "vite build --ssr src/client/client.entry.ts --outDir dist/client",
        "build:api": "vite build --ssr src/api/api.entry.ts --outDir dist/api",
        "build": "npm run build:api && npm run build:client",
        "dev:client": "DOTENV_CONFIG_PATH=.env.local npx vite-node -w src/client/client.entry.ts dist/api/api.entry.js",
        "dev:api": "DOTENV_CONFIG_PATH=.env.local npx vite-node -w src/api/api.entry.ts",
        "dev:api:inspector": "DOTENV_CONFIG_PATH=.env.local npx @modelcontextprotocol/inspector",
        "start": "DOTENV_CONFIG_PATH=.env.local node -r dotenv/config dist/client/client.entry.js dist/api/api.entry.js ",
        "test:unit": "cross-env NODE_ENV=test vitest --reporter=basic",
        "test:unit:coverage": "cross-env NODE_ENV=test vitest run --coverage",
        "docker:dev": "docker compose -f docker-compose.yaml",
        "docker:dev:connect": "npm run docker:dev -- exec mcp-example bash",
        "docker:dev:down": "npm run docker:dev -- down",
        "docker:dev:logs": "npm run docker:dev -- logs -f",
        "docker:dev:up": "npm run docker:dev -- up -d",
        "db:migrate:create": "typeorm-ts-node-esm migration:create src/api/database/migrations/newest-migration-please-rename",
        "db:migrate:generate": "typeorm-ts-node-esm migration:generate -d src/api/database/database.datasource.ts src/api/database/migrations/newest-migration-please-rename",
        "db:migrate": "typeorm-ts-node-esm migration:run -d src/api/database/database.datasource.ts"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --write --ignore-unknown"
        ]
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.53.0",
        "@faker-js/faker": "^8.4.1",
        "@modelcontextprotocol/inspector": "^0.14.0",
        "@modelcontextprotocol/sdk": "^1.12.1",
        "axios": "^1.6.8",
        "cls-hooked": "^4.2.2",
        "clsx": "^2.1.1",
        "continuation-local-storage": "^3.2.1",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "date-fns-timezone": "^0.1.4",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-winston": "^4.2.0",
        "ioredis": "^5.6.1",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "node-cache": "^5.1.2",
        "node-jose": "^2.2.0",
        "pg": "^8.12.0",
        "pino": "^9.5.0",
        "ramda": "^0.29.1",
        "remove": "^0.1.5",
        "typeorm": "^0.3.20",
        "winston": "^3.13.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@types/continuation-local-storage": "^3.2.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/ms": "^0.7.34",
        "@types/node": "^20.12.7",
        "@types/node-cache": "^4.1.3",
        "@types/node-jose": "^1.1.13",
        "@types/ramda": "^0.29.12",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.3.3",
        "chalk": "^5.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "globals": "^15.12.0",
        "husky": "^9.1.6",
        "identity-obj-proxy": "^3.0.0",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.13.0",
        "vite": "^6.3.1",
        "vite-node": "^3.1.1",
        "vite-tsconfig-paths": "^5.1.0",
        "vitest": "^3.1.1"
    }
}
